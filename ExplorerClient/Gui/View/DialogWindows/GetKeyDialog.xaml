<Window x:Class="ExplorerClient.Gui.View.DialogWindows.GetKeyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ExplorerClient.Gui.View.DialogWindows"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="Ключ шифрования"
        Width="444.485"
        Height="91.471"
        Icon="/ExplorerClient;component/Gui/image/key.PNG"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d">
    <Grid>
        <Button x:Name="BtnOk"
                Width="125"
                Height="30"
                Margin="288,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Background="White"
                Click="BtnOk_Click"
                Content="OK" />
        <PasswordBox x:Name="Password"
                     Width="273"
                     Height="30"
                     Margin="10,10,0,0"
                     HorizontalAlignment="Left"
                     VerticalAlignment="Top" />

    </Grid>
</Window>
